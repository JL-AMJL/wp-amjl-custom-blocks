{
    "apiVersion": 2,
    "name": "amjl/icon",
    "title": "Icon",
    "category": "media",
    "icon": "admin-customizer",
    "description": "Ein Icon Block mit fontawesome Icons.",
    "keywords": ["icon", "fontawesome"],
    "supports": {
        "reusable": false
    },
    "attributes": {
        "selectedIcon": {
            "type": "string",
            "default": "ice-cream-solid"
        },
        "iconSize": {
            "type": "string",
            "default": "50px"
        },
        "iconColor": {
            "type": "string",
            "default": "#000000"
        }
    },
    "textdomain": "wp-amjl-custom-blocks",
    "editorScript": "file:../../build/icon.js",
    "style": "file:../../build/icon.css"
}
