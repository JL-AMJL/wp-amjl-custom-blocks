{
    "apiVersion": 2,
    "name": "amjl/icon",
    "title": "Icon",
    "category": "media",
    "icon": "admin-customizer",
    "description": "Ein Icon Block mit fontawesome Icons.",
    "keywords": ["icon", "fontawesome"],
    "supports": {
        "reusable": false,
        "align": ["left", "center", "right"]
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "center"
        },
        "selectedIcon": {
            "type": "string",
            "default": "ice-cream-solid"
        },
        "iconSize": {
            "type": "string",
            "default": "2.0rem"
        },
        "iconColorSlug": {
            "type": "string",
            "default": "contrast"
        }
    },
    "textdomain": "wp-amjl-custom-blocks",
    "editorScript": "file:../../build/icon.js",
    "style": "file:../../build/style-icon.css"
}
